        </div>

        <div>
            <p class="inline-p">Sex Method: @Model.SexMethod</p>
        </div>

        <div>
            <p class="inline-p">Age Method: @Model.AgeMethod</p>
        </div>

        <div>
            <p class="inline-p">Burial Artifact Description: @Model.BurialArtifactDescription</p>
        </div>

        <div>
            <p class="inline-p">Buried With Artifacts: @Model.BuriedWithArtifacts</p>
        </div>
