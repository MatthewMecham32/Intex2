@model Burial



<div>
    <!--gives all the info for the burial selected-->
    <br />
    <br />
    <br />
    <br />
    <br />
    <br />
    <vc:picture />
    <br />
    <br />

    <h1>Additional Information for @Model.BurialId</h1>
    <form asp-action="UploadPhotos" method="get">
        <input type="hidden" asp-for="@Model.OurId" />
        <button class="btn-info" type="submit">Add a picture</button>
    </form>
    <form asp-action="BurialList" method="post">
        <button class="btn-info" type="submit">Burial list</button>
    </form>
    <div class="col-6" style="float:left;">
        <div>
            <p class="inline-p">Face Bundle: @Model.FaceBundle</p>
        </div>

        <div>
            <p class="inline-p">Burial Wrapping: @Model.BurialWrapping</p>
        </div>

        <div>
            <p class="inline-p">Burial Situation: @Model.BurialSituation</p>
        </div>

        <div>
            <p class="inline-p">Cluster: @Model.Cluster</p>
        </div>

        <div>
            <p class="inline-p">Sample Number: @Model.SampleNumber</p>
        </div>

        <div>
            <p class="inline-p">Gender Ge: @Model.GenderGe</p>
        </div>


        <div>
            <p class="inline-p">Gender Body Col: @Model.GenderBodyCol</p>
        </div>

        <div>
            <p class="inline-p">Osteophytosis: @Model.Osteophytosis</p>
        </div>

        <div>
            <p class="inline-p">Robust: @Model.Robust</p>
        </div>

        <div>

            <p class="inline-p">Preservation Index: @Model.PreservationIndex</p>
        </div>

        <div>
            <p>Burial Preservation: @Model.BurialPreservation</p>
        </div>

        <div>
            <p class="inline-p">Hair Taken: @Model.HairTaken </p>
        </div>

        <div>
            <p class="inline-p">Soft Tissue Taken: @Model.SoftTissueTaken </p>
        </div>

        <div>
            <p class="inline-p">Bone Taken: @Model.BoneTaken</p>
        </div>

        <div>
            <p class="inline-p">Tooth Taken: @Model.ToothTaken</p>
        </div>

        <div>
            <p class="inline-p">Textile Taken: @Model.TextileTaken </p>
        </div>

        <div>
            <p class="inline-p">Description Of Taken: @Model.DescriptionOfTaken </p>
        </div>

        <div>
            <p class="inline-p">Artifact Found: @Model.ArtifactFound</p>
        </div>

        <div>
            <p class="inline-p">Estimate Age: @Model.EstimateAge</p>
        </div>

        <div>
            <p class="inline-p">Age Category: @Model.AgeCategory</p>
