<div class="col col-6">
    <form method="post" enctype="multipart/form-data" action="/action_page.php">
        

        <label>Mummy ID: @ViewBag.OurID</label>
        <input type="text" id="id" name="id" value="id"/>

        <label asp-for="PhotoFile">Upload Photo Here</label>
        <input type="file" asp-for="PhotoFile" class="form-control" />

        <label asp-for="Type">Select Photo Type</label>
        <select asp-for="Type" class="form-control">
            <option value="Photo" selected>
                Photo
            </option>
            <option value="idk">
                idk
            </option>
        </select>
        <button asp-action="SavePhotos" class="btn btn-primary" type="submit">Save Photo</button>
    </form>

        
</div>













