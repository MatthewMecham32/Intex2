                                <input type="hidden" name="MonthFound" value="@x.MonthFound" />
                                <input type="hidden" name="DayFound" value="@x.DayFound" />
                                <input type="hidden" name="HeadDirection" value="@x.HeadDirection" />
                                <input type="hidden" name="Gamous" value="@x.Gamous" />
                                <input type="hidden" name="FieldBook" value="@x.FieldBook" />
                                <input type="hidden" name="FieldBookPageNumber" value="@x.FieldBookPageNumber" />
                                <input type="hidden" name="FieldBookNotes" value="@x.FieldBookNotes" />
                                <input type="hidden" name="ByuHasSample" value="@x.ByuHasSample" />
                                <input type="hidden" name="OsteologyUnknownComment" value="@x.OsteologyUnknownComment" />
                                <input type="hidden" name="TemporalMandibularJointOsteoarthritisTmjOa" value="@x.TemporalMandibularJointOsteoarthritisTmjOa" />
                                <input type="hidden" name="PostcraniaAtMagazine" value="@x.PostcraniaAtMagazine" />
                                <input type="hidden" name="LinearHypoplasiaEnamel" value="@x.LinearHypoplasiaEnamel" />
                                <input type="hidden" name="PostcraniaTrauma" value="@x.PostcraniaTrauma" />
                                <input type="hidden" name="Area" value="@x.Area" />
                                <input type="hidden" name="Tomb" value="@x.Tomb" />
                                <input type="hidden" name="Rack" value="@x.Rack" />
                                <input type="hidden" name="Shelf" value="@x.Shelf" />
                                <input type="hidden" name="OsteologyNotes" value="@x.OsteologyNotes" />
                                <input type="hidden" name="SampleTaken" value="@x.SampleTaken" />
                                <input type="hidden" name="SexMethod" value="@x.SexMethod" />
                                <input type="hidden" name="AgeMethod" value="@x.AgeMethod" />
                                <input type="hidden" name="BurialArtifactDescription" value="@x.BurialArtifactDescription" />
                                <input type="hidden" name="BuriedWithArtifacts" value="@x.BuriedWithArtifacts" />
                                <button class="btn btn-primary" type="submit">View Details</button>
                            </form>
                        </div>
                    </div>
                </div>



            }


        </div>
