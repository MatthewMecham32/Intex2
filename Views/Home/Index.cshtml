    <div class="container">

        <!-- Jumbotron Header -->
        <div>
            <div class="container">
                <div class="row">
                    <div class="col">
                        <h1 class="display-3">Fag El-Gamous Mummies</h1>
                        <p class="lead " style="color:gold;">
                            <b>
                                Welcome to our mummy data from the Fag El-Gamous site! On this website, you can filter and look through all the data we have collected on all mummies.
                                Thank you for visiting our website!
                            </b>
                        </p>
                        <a asp-action="BurialList" class="btn btn-primary btn-lg">View Burials</a>
                    </div>
                    <div class="col">
                        <img src="/img/brand.png" />
                    </div>
                </div>




            </div>
            <br />
            <br />
            <br />
            <br />
            <br />
            <br />
            <br />
            <br />



        </div>

        <!-- Page Features -->
        <div class="row text-center">


            <!--Prints 4 items from the thingy-->
            @foreach (var x in Model)
            {


                <div class="col-lg-3 col-md-6 mb-4">
                    <div class="card h-100">
                        <img class="card-img-top" src="/img/brand.png" alt="">
                        <div class="card-body">
                            <h4 class="card-title">@x.BurialId</h4>
                            <p class="card-text">@x.HeadDirection</p>
                            <p class="card-text">@x.GenderGe</p>
                            <p class="card-text">@x.AgeCategory</p>
                        </div>

                        <div class="card-footer">

                            <form id="@x.OurId" method="get" class="btn btn-primary" asp-action="ViewDetails">
                                <input type="hidden" name="BurialId" value="@x.BurialId" />
                                <input type="hidden" name="OurId" value="@x.OurId" />
                                <input type="hidden" name="FaceBundle" value="@x.FaceBundle" />
