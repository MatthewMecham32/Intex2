                                <input type="hidden" name="SoftTissueTaken" value="@x.SoftTissueTaken" />
                                <input type="hidden" name="BoneTaken" value="@x.BoneTaken" />
                                <input type="hidden" name="ToothTaken" value="@x.ToothTaken" />
                                <input type="hidden" name="TextileTaken" value="@x.TextileTaken" />
                                <input type="hidden" name="DescriptionOfTaken" value="@x.DescriptionOfTaken" />
                                <input type="hidden" name="ArtifactFound" value="@x.ArtifactFound" />
                                <input type="hidden" name="EstimateAge" value="@x.EstimateAge" />
                                <input type="hidden" name="AgeCategory" value="@x.AgeCategory" />
                                <input type="hidden" name="EstimateLivingStature" value="@x.EstimateLivingStature" />
                                <input type="hidden" name="ToothAttrition" value="@x.ToothAttrition" />
                                <input type="hidden" name="PathologyAnomalies" value="@x.PathologyAnomalies" />
